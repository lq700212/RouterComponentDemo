<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.componenta">

    <application
        android:name=".AApplication"
        android:allowBackup="true"
        android:icon="@mipmap/a_ic_launcher"
        android:label="@string/a_app_name"
        android:roundIcon="@mipmap/a_ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ComponentDemo">
        <activity
            android:name=".AMainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="com.example.componenta.ACTIVITY" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>